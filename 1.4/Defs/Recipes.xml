<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
	<RecipeDef>
    <defName>Craft_SignalChip</defName>
    <label>craft signal chip</label>
    <description>Craft a signal chip, normally dropped by Diabolus.</description>
    <jobString>Crafting a signal chip.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>4800</workAmount>
    <workSkill>Intellectual</workSkill>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <recipeUsers>
      <li>FabricationBench</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>150</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Gold</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Uranium</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>Plasteel</li>
        <li>Gold</li>
        <li>Uranium</li>
      </thingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>Plasteel</li>
        <li>Gold</li>
        <li>Uranium</li>
      </thingDefs>
    </defaultIngredientFilter>
    <products>
      <SignalChip>1</SignalChip>
    </products>
    <researchPrerequisite>SignalChipCrafting</researchPrerequisite>
    <skillRequirements>
      <Crafting>8</Crafting>
      <Intellectual>14</Intellectual>
    </skillRequirements>
  </RecipeDef>

