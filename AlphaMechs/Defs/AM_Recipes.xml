<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <RecipeDef>
    <defName>Craft_AM_HyperLinkageChip</defName>
    <label>craft hyper-linkage chip</label>
    <description>Craft a hyper-linkage chip (tier 4), normally dropped by Infernus.</description>
    <jobString>Crafting a hyper-linkage chip.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>6000</workAmount>
    <workSkill>Intellectual</workSkill>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <recipeUsers>
      <li>FabricationBench</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>300</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Gold</li>
          </thingDefs>
        </filter>
        <count>30</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>NanostructuringChip</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Uranium</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Plasteel</li>
        <li>Gold</li>
        <li>NanostructuringChip</li>
        <li>Uranium</li>
      </thingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <thingDefs>
        <li>Plasteel</li>
        <li>Gold</li>
        <li>NanostructuringChip</li>
        <li>Uranium</li>
      </thingDefs>
    </defaultIngredientFilter>
    <products>
      <AM_HyperLinkageChip>1</AM_HyperLinkageChip>
    </products>
    <researchPrerequisite>NanostructuringChipCrafting</researchPrerequisite>
    <skillRequirements>
      <Crafting>12</Crafting>
      <Intellectual>18</Intellectual>
    </skillRequirements>
  </RecipeDef>

